<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="456.0" prefWidth="673.0" style="-fx-background-color: #AED6F1;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.login.app.controller.tutorialwindow.TutorialController">

   <children>
      <Button fx:id="previousButton" layoutX="116.0" layoutY="288.0" mnemonicParsing="false" onAction="#changeLabelText" prefHeight="57.0" prefWidth="121.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%;" text="Previous tip" />
      <Button fx:id="nextButton" layoutX="449.0" layoutY="288.0" mnemonicParsing="false" onAction="#changeLabelText" prefHeight="57.0" prefWidth="121.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%; -fx-border-width: 3px;" text="Next tip" />
      <Label fx:id="editableLabel" alignment="CENTER" layoutX="32.0" layoutY="57.0" prefHeight="183.0" prefWidth="610.0" text="Welcome to the sql murder game!">
         <font>
            <Font name="Berlin Sans FB" size="17.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="290.0" layoutY="360.0" mnemonicParsing="false" onAction="#changeToGameWindow" prefHeight="57.0" prefWidth="121.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%; -fx-border-width: 3px;" text="Go Play!" />
   </children>
</AnchorPane>
