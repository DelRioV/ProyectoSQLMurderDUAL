<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="524.0" prefWidth="767.0" style="-fx-background-color: #AED6F1;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.login.app.controller.tutorialwindow.TutorialController">

   <children>
      <Button fx:id="previousButton" layoutX="148.0" layoutY="288.0" mnemonicParsing="false" onAction="#changeLabelText" prefHeight="76.0" prefWidth="143.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%;" text="Previous tip" />
      <Button fx:id="nextButton" layoutX="518.0" layoutY="288.0" mnemonicParsing="false" onAction="#changeLabelText" prefHeight="76.0" prefWidth="143.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%; -fx-border-width: 3px;" text="Next tip" />
      <Label fx:id="editableLabel" alignment="CENTER" layoutX="20.0" layoutY="61.0" prefHeight="183.0" prefWidth="723.0" text="Welcome to the sql murder game!">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="338.0" layoutY="364.0" mnemonicParsing="false" onAction="#changeToGameWindow" prefHeight="76.0" prefWidth="143.0" style="-fx-background-color: #EAF2E3; -fx-background-radius: 12%; -fx-border-radius: 12%; -fx-border-width: 3px;" text="Go Play!" />
   </children>
</AnchorPane>
